{
	"description": "Command Line Client for Google Drive",
	"name": "digitalzenworks/google-drive-cli",
	"license": "proprietary",
	"type": "project",
	"authors": [
		{
			"name": "JamesMc",
			"email": "jamesjohnmcguire@gmail.com"
		}
	],
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		},
		"process-timeout": 0,
		"vendor-dir": "SourceCode/vendor"
	},
	"require": {
		"php": "^7.4 || ^8.0",
		"google/apiclient": "^2.11",
		"digitalzenworks/google-api-authorization": "^1.0"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "*",
		"php-parallel-lint/php-console-highlighter": "^1.0",
		"php-parallel-lint/php-parallel-lint": "^1.2.0",
		"phpcompatibility/php-compatibility": "*",
		"phpcsstandards/phpcsdevtools": "^1.0",
		"phpcsstandards/phpcsextra": "^1.2.0",
		"phpmd/phpmd": "^2.15",
		"phpstan/extension-installer": "^1.4",
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-deprecation-rules": "^2.0",
		"phpunit/phpunit": "^12.0",
		"roave/security-advisories": "dev-master",
		"squizlabs/php_codesniffer": "*"
	}
}
